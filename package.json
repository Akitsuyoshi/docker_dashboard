{
  "name": "docker_dashboard",
  "version": "1.0.0",
  "description": "docker visualization",
  "main": "./bin/www/index.js",
  "scripts": {
    "start": "NODE_ENV=production babel-node ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "server": "nodemon ./bin/www --exec babel-node",
    "client": "webpack-dev-server ---mode development --open",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "test": " jest --watchAll --coverage",
    "coverage": "jest --forceExit --coverage --coverageReporters=\"text-lcov\" > coverage.lcov",
    "test-ci": "jest && yarn coverage"
  },
  "author": "Akitsuyoshi:ka090824@gmail.com",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --debug --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=11.1",
    "yarn": ">= 1.9.4"
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "auth0-js": "9.8.2",
    "axios": "0.18.0",
    "babel-core": "^7.0.0-bridge",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "enzyme-adapter-react-16": "1.7.1",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "graphql": "14.0.2",
    "mongoose": "5.4.0",
    "parse-error": "0.2.0",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-materialize": "2.6.0",
    "react-redux": "6.0.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "socket.io": "2.4.0",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-polyfill": "6.26.0",
    "codecov": "3.1.0",
    "concurrently": "4.0.1",
    "css-loader": "1.0.1",
    "enzyme": "3.8.0",
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "morgan": "1.9.1",
    "node-sass": "4.9.4",
    "nodemon": "1.18.8",
    "prettier": "1.14.3",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "4.3.12",
    "react-test-renderer": "16.7.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.24.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-dev-server": "3.1.10",
    "webpack-hot-middleware": "2.24.3"
  }
}
